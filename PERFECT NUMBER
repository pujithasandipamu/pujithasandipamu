#include <stdio.h>
int main() 
{
    int n,sum=0;
    int i;
    printf("enter any number");
    scanf("%d",&n);
    for(i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
    }
        if (sum==n)
        {
            printf("perfect");
        }
        else
        {
            printf("not perfect");
        }
}
        
    
