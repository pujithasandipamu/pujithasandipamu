#include<stdio.h>
void swap(int *a,int *b);
int main()
{

    int a,b;
    printf("enter any two numbers");
    scanf("%d %d",&a,&b);
    printf("\n before swapping the values are %d %d",a,b);
    swap(&a,&b);
    printf("\n after swapping the values are %d %d",a,b);
    return 0;
}
void swap(int*p,int*q)
{
    int t;
    t=*p;
    *p=*q;
    *q=t;
}

