#include<stdio.h>
#include<string.h>
void main()
{
    char a[10];
    char b[10];
    char c[10];
     printf("Enter the name 1:");
     scanf("%s",a);
     printf("Enter the name 2:");
     scanf("%s",b);
printf("Enter the name 3:");
     scanf("%s",c);

    for(int i=0;i<5;i++)
    {
        printf("   %c",a[i]);
    }
    printf("\n");
    for( int i=0;i<5;i++)
    {
        printf("   %c",b[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
        printf("   %c",c[i]);
    }
      printf("\n");
}
