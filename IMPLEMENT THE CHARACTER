#include<stdio.h>
void main()
{

    char a[10];
    char b[10]="ROHIT";
    char c[10]="VIJAY";
    for(int i=0;i<5;i++)
    {
        printf("%c",b[i]);
    }
        {
        printf("\n");
        for(int i=0;i<5;i++)
        {
            printf("%c",c[i]);
        }
        printf("\n");
    }
}
