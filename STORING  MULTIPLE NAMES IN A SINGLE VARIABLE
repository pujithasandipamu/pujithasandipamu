#include<stdio.h>
void main()
{
    char name[10][10];
    int a;
    printf("Enter no.of names:");
    scanf("%d",&a);
    for(int i=0;i<a;i++)
    {
        scanf("%s",name[i]);
    }
    printf("\nThe names are as follows...");
    for(int i=0;i<a;i++)
    {
        printf("\nname%d.%s",i+1,name[i]);
    }
}
